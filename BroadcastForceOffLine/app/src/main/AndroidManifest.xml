<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.ics.broadcastforceoffline">

    <uses-permission android:name="android.permission.SYSTEM_ALERT_WINDOW">

    </uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.project.ics.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>


        </activity>
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name="com.project.ics.activities.BaseActivity">

        </activity>
        <receiver android:name="com.project.ics.mybroadcast.ForceOfflineReceiver">
            <intent-filter>
                <action android:name="com.example.FORCE_OFFLINE">

                </action>
            </intent-filter>
        </receiver>
    </application>

</manifest>